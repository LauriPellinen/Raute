<style>
    .fullScreen {
        width: 100%;
        height: 100%;
        position: absolute;
        top: 0;
        left: 0;
    }
    iframe {
        display: block;
        width: 100%;
        border: none;
        overflow-x: hidden;
        overflow-y: auto;
    }
    .zoomIn {
        -webkit-user-select: none;
        margin: auto;
        cursor: zoom-in;
        background-color: hs1(0, 0%, 90%);
        transition: background-color 300ms;
    }
    .zoomOut {
        -webkit-user-select: none;
        margin: auto;
        cursor: zoom-out;
        background-color: hs1(0, 0%, 90%);
        transition: background-color 300ms;
    }
</style>
